{
    "name": "smetaniny/sm-laravel-image",
    "description": ":Обработка изображений laravel",
    "keywords": ["image", "imagick", "laravel", "watermark", "thumbnail"],
    "license": "MIT",
    "authors": [
        {
            "name": "Smetanin Sergey",
            "email": "sm.sergey.v@yandex.ru",
            "homepage": "https://smetaniny.ru/"
        }
    ],
    "homepage": "https://github.com/smetaniny/sm-laravel-image",
    "keywords": [
        "Laravel",
        "LaravelImage"
    ],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Smetaniny\\LaravelImage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smetaniny\\LaravelImage\\Tests\\": "tests",
            "Smetaniny\\LaravelImage\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.1-dev"
        },
        "laravel": {
            "providers": [
                "Smetaniny\\LaravelImage\\LaravelImageServiceProvider"
            ],
            "aliases": {
                "Image": "Smetaniny\\LaravelImage\\Facades\\ImageFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
